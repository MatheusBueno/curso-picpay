{
  "env": {
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "react-native", "jsx-a11y", "import", "prettier"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off"
  }
}
